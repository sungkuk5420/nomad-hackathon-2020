{
    "type": "chrome",
    "request": "launch",
    "name": "Quasar App: chrome",
    "url": "http://localhost:8080",
    "webRoot": "${workspaceFolder}/src",
    "breakOnLoad": true,
    "sourceMapPathOverrides": {
        "webpack:///./src/*": "${webRoot}/*"
    },
    "configurations": [
        {
            "type": "pwa-chrome",
            "name": "http://localhost:8080",
            "request": "launch",
            "url": "http://localhost:8080"
        }
    ]
}